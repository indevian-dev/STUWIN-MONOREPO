{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/types": [
        "../_shared.types/index"
      ],
      "@/types/auth/*": [
        "../_shared.types/auth/*"
      ],
      "@/types/domain": [
        "../_shared.types/domain/index"
      ],
      "@/types/domain/*": [
        "../_shared.types/domain/*"
      ],
      "@/types/*": [
        "../_shared.types/*"
      ]
    }
  }
}

